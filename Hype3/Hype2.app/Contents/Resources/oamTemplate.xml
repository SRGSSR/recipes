<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://openajax.org/metadata" spec="1.0" id="tag:adobe.com,2012-03:edge:oam:compid:${documentName}" name="${documentName}" version="1">

  <description type="text/html"><![CDATA[Widget Exported from Tumult Hype]]></description>
  <category name="Adobe Edge Animation"/>

  <remarks>Compatible with: InDesign CC, Dreamweaver CC, Muse</remarks>

  <icon src="Default.png" width="${width}" height="${height}" />
  
  ${requires}

  <content><![CDATA[
    <div></div>
  ]]></content>
   
  <properties name="Properties">
    <property name="default-width" defaultValue="${width}" hidden="true"/>
    <property name="default-height" defaultValue="${height}" hidden="true"/>
    <property name="resizable" defaultValue="${isResizable}" hidden="true"/>
    <property name="use-chrome-frame" defaultValue="false" hidden="true"/>
    <property name="chrome-frame-installer" defaultValue="" hidden="true"/>				
		<property name="centering" defaultValue="none" hidden="true"/>				
		<property name="scaling" defaultValue="none" hidden="true"/>
    <property name="html-page" defaultValue="${htmlFileName}" hidden="true"/>
  </properties>

</widget>
